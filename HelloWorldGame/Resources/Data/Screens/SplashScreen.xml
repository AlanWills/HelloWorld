<?xml version="1.0" encoding="utf-8"?>
<Level>
  <Resources>
    <Texture2Ds>
      <Texture2D>Sprites\UI\CelesteGamesLogo.png</Texture2D>
      <Texture2D>Sprites\UI\CelesteEngineLogo.png</Texture2D>
    </Texture2Ds>
  </Resources>
  <GameObjects>
    <GameObject layer="GUI" position="0.5%" name="Logo0">
      <SpriteRenderer path="Sprites\UI\CelesteGamesLogo.png" size="0.9%" opacity="0"/>
      <OpacityLerper lerping_up="true" lerp_down_time="1" lerp_up_time="1" min_wait_time="0.2" max_wait_time="1"/>
      <LimitedLifeTime lifetime="3.2">
        <Callback name="setActive" arguments="Logo1"/>
        <Callback name="setShouldRender" arguments="Logo1"/>
      </LimitedLifeTime>
    </GameObject>
    <GameObject layer="GUI" position="0.5%" name="Logo1" active="false" should_render="false">
      <SpriteRenderer path="Sprites\UI\CelesteEngineLogo.png" size="0.9%" opacity="0"/>
      <OpacityLerper lerping_up="true" lerp_down_time="1" lerp_up_time="1" min_wait_time="0.2" max_wait_time="1"/>
      <LimitedLifeTime lifetime="3.2">
        <Callback name="killOwnerScreen"/>
      </LimitedLifeTime>
    </GameObject>
  </GameObjects>
</Level>